{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true
  },
  "plugins": ["import", "react"],
  "extends": ["eslint:recommended", "plugin:import/errors", "plugin:react/recommended"],
  "rules": {
    "no-unused-vars": [2, { "ignoreRestSiblings": true }],
    "import/no-unresolved": 0,
    "react/prop-types": 0,
    "react/prefer-stateless-function": 1
  },
  "globals": {
    "__DEV__": true
  },
  "settings": {
    "react": {
      "version": "16"
    }
  }
}
